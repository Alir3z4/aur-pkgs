#!/bin/sh
# Shell Script to Execute server file witch is locate in
# /usr/share/0verkill-0.16 directory. 
# Script Author: Bartosz Dusilo <cbreaker@tlen.pl>
DIR=`pwd`
echo "Entering to /usr/share/0verkill-0.16 directory"
cd /usr/share/0verkill-0.16 
echo "Running 0verkill - Have Fun"
exec ./server
# Back to pwd directory.
cd $DIR
