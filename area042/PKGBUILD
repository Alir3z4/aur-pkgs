pkgname=area042
pkgver=0.42
pkgrel=1
pkgdesc="This is a SVG-Icon Theme inspired by heylove's areao.42-Icons."
arch=('any')
url="http://www.gnome-look.org/content/show.php/Kamel+icons?content=59006"
license=('GPL')
source=(http://www.gnome-look.org/CONTENT/content-files/78259-area042ELEGANT.tar.bz2)
md5sums=('f755887f5aaf8f9ccb82ac0deb41cce1')

package() {
    cd $startdir/src/
    mkdir -p $startdir/pkg/usr/share/icons/
    cp -r "BRIT ICONS" $startdir/pkg/usr/share/icons/
}

